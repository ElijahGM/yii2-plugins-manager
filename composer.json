{
    "name": "esoftslimited/yii2-plugins-manager",
    "description": "Plugins system as WP for Yii2 copied from loveorigami for learning purpose",
    "type": "library",
    "keywords": ["yii2","plugins","events","managers"],
    "license": "MIT",
    "authors": [
        {
            "name": "Elijah Mwangi",
            "email": "emwangi.g@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.6",
        "components/highlightjs":"*",
        "loveorigami/yii2-jsoneditor": "*",
        "tpoxa/shortcodes": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "esoftslimited\\plugins\\": ""
        }
    },
    "extra": {
        "bootstrap": "esoftslimited\\plugins\\Bootstrap",        
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
